{
   "type": "postgres",
   "url": "postgresql://docker:docker@postgres/server-graphql_dev",
  "synchronize": true,
  "logging": false,
  "entities": [
     "src/database/entity/**/*.ts"
  ],
  "migrations": [
     "src/database/migration/**/*.ts"
  ],
  "subscribers": [
     "src/subscriber/**/*.ts"
  ]
}
